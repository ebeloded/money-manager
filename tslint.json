{
  "extends": ["tslint-config-prettier", "tslint-react"],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "no-console": [false],
    "jsx-no-multiline-js": [false],
    "no-var-requires": [false],
    "no-empty-interface": [false]
  },
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  }
}
