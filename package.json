{
  "name": "money-manager",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tslint:fix": "tslint -p . --fix --force",
    "tslint:watch": "chokidar './src/**/*.ts' './src/**/*.tsx'  -c 'tslint -p . --fix --force'",
    "tslint:fix:watch": "yarn run tslint:fix && yarn run tslint:watch",
    "tsc:watch": "tsc --project ./tsconfig.json --noEmit --watch",
    "start-js": "react-scripts-ts start",
    "build-js": "react-scripts-ts build",
    "build-css": "node-sass-chokidar src/styles -o src/styles --include-path ./node_modules",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/styles --watch --recursive --include-path ./node_modules",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "@material/button": "^0.35.0",
    "@material/react-button": "^0.1.0",
    "bootstrap": "^4.1.1",
    "debug": "^3.1.0",
    "emotion": "^9.1.3",
    "emotion-theming": "^9.1.2",
    "firebase": "^5.0.3",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.1.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "rmwc": "^1.6.3",
    "rxjs": "^6.1.0",
    "tslib": "^1.9.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/chance": "^1.0.0",
    "@types/debug": "0.0.30",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.108",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.0",
    "@types/react-router-dom": "^4.2.6",
    "@types/uuid": "^3.4.3",
    "chance": "^1.0.16",
    "concurrently": "^3.5.1",
    "husky": "^0.14.3",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1",
    "react-scripts-ts": "^2.15.1",
    "rxjs-tslint": "^0.1.3",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.10.0",
    "why-did-you-update": "^0.1.1"
  }
}
